$:stack(
s("bd*4,- [cp,sd]").bank("RolandTR909"),
chord("[- g:minor] - [- g:major] -")
.add(note("0,.31"))
.s("supersaw:2").n(irand(8))
.cutoff(perlin.range(4100,3556).slow(4))
.decay(perlin.range(.2,.3)).sustain(0.1)
.lpq(10)
.room(slider(0.468))
.sz(slider(6,0,10,.5))
.rfade(slider(0.462))
.roomdim(slider(0.349)))
.duck(irand(1.3)).duckdepth(.4).duckattack(irand(1))
